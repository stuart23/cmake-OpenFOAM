FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      rotatingZoneProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

origin    (0 0 0);
axis      (0 0 1);
omega     constant <rad/s>;
nonRotatingPatches (fixed);

