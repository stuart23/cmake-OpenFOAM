FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      cartesianYZ;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type      cartesian;
origin    (0 0 0);
coordinateRotation
{
    type  axesRotation;
    e2    $y;
    e3    $z
}

